{
  "expo": {
    "name": "Little Ape Movies",
    "scheme": "littleapemovies",
    "userInterfaceStyle": "automatic",
    "orientation": "portrait",
    "version": "1.2.02",
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "icon": "./assets/images/papericon2.png",
    "splash": {
      "image": "./assets/papersplashscreen2.png",
      // "imageWidth": 1284,
      "resizeMode": "contain",
      "backgroundColor": "#544D6C",
      "enableFullScreenImage_legacy": true
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true
          }
        }
      ],
      [
        "expo-router",
        {
          "origin": "https://n"
        }
      ],
      "expo-font",
      "expo-secure-store",
      [
        "expo-splash-screen",
        {
          "ios": {
            // You may want to use regular full screen splash on iOS 
            "image": "./assets/papersplashscreen2.png", // Full screen transparent splash png
            "enableFullScreenImage_legacy": true,
            "resizeMode": "contain",
            "backgroundColor": "#544D6C"
          }
        }
      ]
    ],
    "newArchEnabled": true,
    "slug": "little-ape-movies",
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.markmccoid.littleapemovies"
    },
    "ios": {
      "bundleIdentifier": "com.markmccoid.littleapemovies"
    },
    "extra": {
      "router": {
        "origin": "https://n"
      },
      "eas": {
        "projectId": "b7df56aa-9e6f-40c6-8e4b-91ccbc5c4596"
      }
    }
  }
}
