{
  "expo": {
    "scheme": "littleapemovies",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "version": "1.0.5",
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "icon": "./assets/images/appicon.png",
    "splash": {
      "image": "./assets/images/StartupScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true
          }
        }
      ],
      [
        "expo-router",
        {
          "origin": "https://n"
        }
      ],
      "expo-font",
      "expo-secure-store"
    ],
    "name": "little-ape-movies",
    "slug": "little-ape-movies",
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.markmccoid.littleapemovies"
    },
    "ios": {
      "bundleIdentifier": "com.markmccoid.littleapemovies"
    },
    "extra": {
      "router": {
        "origin": "https://n"
      },
      "eas": {
        "projectId": "b7df56aa-9e6f-40c6-8e4b-91ccbc5c4596"
      }
    }
  }
}
