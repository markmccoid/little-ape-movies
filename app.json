{
  "expo": {
    "scheme": "littleapemovies",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "icon": "./assets/images/appicon.png",
    "splash": {
      "image": "./assets/images/StartupScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      ["expo-build-properties", {
        "ios": { "newArchEnabled" : true}
      }],
      [
        "expo-router",
               {
          "origin": "https://n"
        }
      ],
      "expo-font",
      "expo-secure-store"
    ],
    "name": "little-ape-movies",
    "slug": "little-ape-movies",
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.markmccoid.littleapemovies"
    },
    "ios": {
      "bundleIdentifier": "com.markmccoid.littleapemovies"
    }
  }
}
